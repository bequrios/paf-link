@prefix : <https://example.com/> .
@prefix paf: <https://paf.link/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterm: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:registration-activty a prov:Activity.

:3280-name-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:name;
    schema:name "Wissenschaftliches Potenzial für Krisenzeiten nutzen (Michel)"@de.

:3280-identifier-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:identifier;
    schema:identifier "2020 P 20.3280".

:3280-description-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:description;
    schema:description "Der Bundesrat wird beauftragt, in einem Bericht zu prüfen, wie ein interdisziplinäres..."@de.

:3542-name-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:name;
    schema:name "Ein Kompetenzzentrum für die Zeit nach Covid-19 (de Quattro)"@de.

:3542-identifier-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:identifier;
    schema:identifier "2022 P 20.3542".

:3542-description-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:description;
    schema:description "Der Bundesrat wird beauftragt, in einem Bericht die Schaffung eines Kompetenzzentrums..."@de.

:proposal-creation-activity a prov:Activity;
    prov:wasInformedBy :registration-activty;
    prov:used :3280-name-entity, :3280-identifier-entity, :3280-description-entity, :3542-name-entity, :3542-identifier-entity, :3542-description-entity.

:answer-entity a prov:Entity;
    prov:wasGeneratedBy :answer-creation-activity;
    rdf:predicate chpaf:mopoAnswer;
    chpaf:mopoAnswer "Postulatsbericht vom 23. November 2022 «Wissenschaftliches Potenzial für Krisenzeiten nutzen»."@de;
    chpaf:report <https://www.newsd.admin.ch/newsd/message/attachments/74007.pdf>.

:proposal-entity a prov:Entity;
    prov:wasGeneratedBy :proposal-creation-activity;
    rdf:predicate chpaf:proposal;
    chpaf:proposal chpaf:Abandonment;
    schema:description "Der Bundesrat erachtet das Anliegen der Postulate als erfüllt und beantragt deren Abschreibung."@de.

:proposal-activity a prov:Activity, chpaf:ProposalForAbandonmentActivity;
    prov:wasInformedBy :answer-creation-activity;
    prov:qualifiedAssciation [
        a prov:Association;
        prov:agent <https://ld.admin.ch/FC>;
        prov:hadRole paf:ProposalSubmitter;
    ];
    prov:used :3280-name-entity, :3280-identifier-entity, :3280-description-entity, :3542-name-entity, :3542-identifier-entity, :3542-description-entity, :answer-entity, :proposal-entity.