@prefix : <https://example.com/> .
@prefix paf: <https://paf.link/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterm: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:registration-activty a prov:Activity.

:name-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:name;
    schema:name "Anlaufstelle(n) in Sachen Administrativ- und Disziplinaruntersuchungen Geschäftsprüfungskommission NR)"@de.

:identifier-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:identifier;
    schema:identifier "2020 M 19.4390".

:description-entity a prov:Entity;
    prov:wasCreatedBy :registration-activty;
    rdf:predicate schema:description;
    schema:description "Der Bundesrat wird beauftragt, eine oder mehrere Stellen zu bezeichnen..."@de.

:proposal-creation-activity a prov:Activity;
    prov:wasInformedBy :registration-activty;
    prov:used :name-entity, :identifier-entity, :description-entity.

:answer-entity a prov:Entity;
    prov:wasGeneratedBy :answer-creation-activity;
    rdf:predicate chpaf:mopoAnswer;
     chpaf:mopoAnswer "Die am 1. Januar 2022 in Kraft getretenen Weisungen des Bundesrates..."@de.

:proposal-entity a prov:Entity;
    prov:wasGeneratedBy :proposal-creation-activity;
    rdf:predicate chpaf:proposal;
    chpaf:proposal chpaf:Abandonment;
    schema:description "Der Bundesrat erachtet das Anliegen der Motion als erfüllt und beantragt deren Abschreibung."@de.

:proposal-activity a prov:Activity, chpaf:ProposalForAbandonmentActivity;
    prov:wasInformedBy :answer-creation-activity;
    prov:qualifiedAssciation [
        a prov:Association;
        prov:agent <https://ld.admin.ch/FC>;
        prov:hadRole paf:ProposalSubmitter;
    ];
    prov:used :name-entity, :identifier-entity, :description-entity, :answer-entity, :proposal-entity.